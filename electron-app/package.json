{
  "name": "svg-2-jsx",
  "version": "1.0.0",
  "description": "Get in SVG get out react",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/saravieira/svg-2-jsx.git"
  },
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "main": "main.js",
  "scripts": {
    "prod": "webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "NODE_OPTIONS=\"--max-old-space-size=4096\" webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds --overwrite --icon=./icons/mac/app.icns",
    "lint": "eslint src",
    "pretest": "npm run lint"
  },
  "dependencies": {
    "@svgr/core": "^4.3.2",
    "@svgr/plugin-jsx": "^4.3.2",
    "@svgr/plugin-prettier": "^4.3.2",
    "@svgr/plugin-svgo": "^4.3.1",
    "@tippy.js/react": "^2.2.3",
    "copy-to-clipboard": "^3.2.0",
    "electron": "^6.0.7",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "^1.18.2",
    "prism-react-renderer": "^0.1.7",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-dropzone": "^10.1.8",
    "reakit": "^1.0.0-beta.6",
    "standard": "^14.3.0",
    "styled-components": "^4.3.2",
    "svgo": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babili-webpack-plugin": "^0.1.2",
    "electron-packager": "^14.0.5",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
